<configuration>
  <configSections>
    <!--SD.Toolkits.SerialNumber配置节点-->
    <section name="sd.toolkits.serialNumber" type="SD.Toolkits.SerialNumber.SerialNumberSection, SD.Toolkits.SerialNumber" />
  </configSections>
  <!--SD.Toolkits.SerialNumber配置-->
  <sd.toolkits.serialNumber>
    <!--序列种子提供者-->
    <serialSeedProvider type="SD.Toolkits.SerialNumber.Implements.DefaultSerialSeedProvider" assembly="SD.Toolkits.SerialNumber" />
    <!--连接字符串，Name/Value二选一，Name优先-->
    <connectionString name="SerialNumberConnection" value="Data Source=.;Initial Catalog=SerialNumber;User Id=sa;Password=123456;MultipleActiveResultSets=true;" />
  </sd.toolkits.serialNumber>
</configuration>