<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net45</TargetFramework>
    <RootNamespace>RabbitMQ.ServiceModel</RootNamespace>
    <Authors>Lee</Authors>
    <Company>Pivotal Software, Inc.</Company>
    <Description>RabbitMQ Binding for Windows Communication Foundation With Load Balance. Powered by Lee.</Description>
    <Copyright>Copyright © 2007-2020 Pivotal Software, Inc.</Copyright>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Debug\$(TargetFramework)\SD.RabbitMQ.ServiceModel.xml</DocumentationFile>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>bin\Release\$(TargetFramework)\SD.RabbitMQ.ServiceModel.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="System.Configuration" />
    <Reference Include="System.ServiceModel" />
    <PackageReference Include="RabbitMQ.Client" Version="3.6.9" />
  </ItemGroup>
</Project>
